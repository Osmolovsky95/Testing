<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>AddServlet</servlet-name>
        <servlet-class>AddServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>ListServlet</servlet-name>
    <servlet-class>ListServlet</servlet-class>
</servlet>

    <servlet-mapping>
        <servlet-name>ListServlet</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>DeleteStudent</servlet-name>
    <servlet-class>DeleteServlet</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>DeleteStudent</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Administrator</servlet-name>
        <servlet-class>AdministrarorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Administrator</servlet-name>
        <url-pattern>/administratorPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateQuestion</servlet-name>
        <servlet-class>QuestionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateQuestion</servlet-name>
        <url-pattern>/createQuestion</url-pattern>
    </servlet-mapping>

</web-app>